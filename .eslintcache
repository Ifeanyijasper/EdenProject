ntact: {Hyphenated(worker.phone)}</p>\n            </div>\n           {/* <MiniProgressBar progress={worker.friend_name || 0 } /> */}\n        </div>\n    )\n}\n\nexport default AdminCard;\n","/home/eden/EdenProject/src/components/WorkArea/WorkArea.component.js",[],"/home/eden/EdenProject/src/components/Search/Search.component.js",["578","579","580","581","582"],"import React, { useEffect, useState } from 'react';\nimport { IoAdd, IoAddCircle, IoAddCircleOutline, IoChevronDown, IoChevronUp, IoSearch } from 'react-icons/io5';\nimport { Activity } from '..';\n\nimport styles from './Search.module.css';\n\nconst Search = (props) => {\n    const {\n        placeholder,\n        setIsOpen,\n        newButton,\n        title,\n        filters,\n        filter,\n        setFilter,\n        setText,\n        purchase,\n        onClick,\n    } = props;\n    const [showList, setShowList] = useState(false);\n    const [search, setSearch] = useState(false);\n    \n    useEffect(() => {\n        setFilter(filters[0])\n    }, [])\n\n    const setFilterValue = (index) => {\n        setFilter(index);\n        setShowList(false)\n        setSearch(true);\n    }\n\n    const filterData = (event) => {\n        setText(event.target.value);\n    }\n\n    return (\n        <div className={`flex justify-between -mx-1 md:mx-4 items-center mb-3`}>\n            <div className=\"ml-auto flex items-center bg-gray-300 shadow-lg rounded-3xl p-2 mr-3 md:mr-4\">\n                <input autoFocus type=\"text\" placeholder={placeholder + ' By:' + filter} onChange={(event) => filterData(event)} className={`outline-none text-xs md:text-sm text-gray-800 bg-transparent transition-all duration-500 ease-in-out transform ${search ? 'w-28 md:w-32 lg:w-40 pl-3 translate-x-0 opacity-100 focus' : 'w-0 translate-x-6 opacity-0'}`} />\n                <IoSearch className=\"text-gray-900 cursor-pointer mx-2 z-10\" onClick={() => setSearch(!search)} />\n            </div>\n            <div className={`relative`}>\n                <button className={'outline-none flex items-center bg-primary rounded-3xl text-white p-1.5 px-5 text-xs md:text-sm font-semibold transition duration-500 ease-in-out hover:shadow-xl'} onClick={() => setShowList(!showList)}>{filter} {showList ? <IoChevronUp className=\"ml-2\" /> : <IoChevronDown className=\"ml-2\" />}</button>\n                <ul className={`absolute z-10 top-11 right-0 bg-white w-36 rounded py-1 shadow-lg ${showList ? 'transition duration-500 ease-in opacity-100 visible' : 'transition-all duration-500 opacity-0 invisible'}`}>\n                    {filters.map((_filter, index) => (\n                        <li className={`cursor-pointer block text-sm text-gray-800 p-4 py-1 transition duration-500 ease-in-out hover:bg-green-200`} onClick={() => setFilterValue(_filter)}>{_filter}</li>\n                    ))}\n                </ul>\n            </div>\n            {newButton && (\n                    <button className={'flex outline-none ml-2 items-center justify-center text-sm text-primary bg-white shadow-md p-1.5 md:px-4 rounded-3xl text-white transition duration-500 ease-in-out hover:shadow-2xl'} onClick={() => setIsOpen(true)}><IoAddCircleOutline className=\"md:mr-1.5\" /> <span className=\"hidden md:inline-block\">{title}</span></button>\n            )}\n            {purchase && (\n                    <button className={'flex outline-none ml-2 items-center justify-center text-sm text-primary bg-white shadow-md p-1.5 md:px-4 rounded-3xl text-white transition duration-500 ease-in-out hover:shadow-2xl'} onClick={() => onClick()}><IoAddCircleOutline className=\"md:mr-1.5\" /> <span className=\"hidden md:inline-block\">{title}</span></button>\n            )}\n        </div>\n    )\n};\n\nexport default Search;\n","/home/eden/EdenProject/src/components/MiniProgressBar/MiniProgessBar.component.js",[],"/home/eden/EdenProject/src/components/MiniProgressBar/Progress.component.js",[],"/home/eden/EdenProject/src/components/ClientsCard/ClientsC